<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="28dip" />
            <stroke android:width="1dip" android:color="#69B3BB" />
            <gradient android:angle="-90" android:startColor="#69B3BB" android:endColor="#69B3BB"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="28dip" />
            <stroke android:width="1dip" android:color="#69B3BB" />
            <solid android:color="#69B3BB"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="28dip" />
            <stroke android:width="1dip" android:color="#69B3BB" />
            <gradient android:angle="-90" android:startColor="#69B3BB" android:endColor="#69B3BB" />
        </shape>
    </item>
</selector>